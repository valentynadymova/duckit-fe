{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "quotes": ["error", "single"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  }
}
